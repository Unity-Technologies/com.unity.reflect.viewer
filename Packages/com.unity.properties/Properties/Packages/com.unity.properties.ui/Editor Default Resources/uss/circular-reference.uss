.unity-properties__circular-reference__icon
{
    width:16px;
    height:16px;
    border-width: 0;
    align-self: center;
    background-color: transparent;
    background-image: var(--circular-reference-icon);
}

.unity-properties__circular-reference__path
{
    padding-left: 5px;
