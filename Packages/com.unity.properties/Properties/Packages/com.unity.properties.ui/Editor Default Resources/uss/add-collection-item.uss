.unity-properties__add-dictionary-key__container
{
    background-color: var(--new-key-item__color);
    border-radius: 5px;
    margin-left: -15px;
    margin-top: 1px;
    padding-left: 15px;
    padding-top: 2px;
}

.unity-properties__add-dictionary-buttons-root
{
    align-self: flex-end;
    flex-direction: row;
}

.unity-properties__add-dictionary-key__key-root
{
    flex-direction: row;
}

.unity-properties__add-dictionary-key__key-element
{
    flex-grow: 1;
}

.unity-properties__add-dictionary-key__show-container-button
{
    align-self: flex-end;
    background-color: transparent;
    border-width: 0;
}

.unity-properties__add-dictionary-key__add-button
{
    align-self: flex-end;
    background-color: transparent;
    border-width: 0;    
}

.unity-properties__add-dictionary-key__cancel-button
{
    background-color: transparent;
    background-image: var(--remove-button);
    border-width: 0;
    max-height: 16px;
    max-width: 16px;
}

.unity-properties__add-dictionary-key__error-info
{
    background-color: transparent;
    background-image: var(--error-icon);
    border-width: 0;
    max-height: 16px;
    max-width: 16px;
}
